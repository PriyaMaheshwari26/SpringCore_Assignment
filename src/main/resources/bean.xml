    <bean id="address" class="solution1.Address">
        <property name="street" value="JASA"/>
        <property name="state" value="Sirasoul"/>
        <property name="zip" value="243633"/>
        <property name="country" value="India"/>
    </bean>

    <bean id="collection" class="solution10.AppAware">
        <property name="list">
            <list>
                <value>John</value>
                <value>Nick</value>
                <value>Thomas</value>
                <value>Bob</value>
            </list>
        </property>
    </bean>

    <bean id="question" class="solution2.Question">
        <property name="questionId" value="Q1"/>
        <property name="question" value="What is Spring Core?"/>
        <property name="answerList">
            <list>
                <value>The Spring Framework is an open-source framework for building enterprise Java applications</value>
                <value>Spring is a lightweight framework.</value>
            </list>
        </property>
    </bean>

</beans>